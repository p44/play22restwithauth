# Routes
# ~~~~

GET     /                           controllers.Register.index

GET     /customer/:id/callbackregistration       controllers.Register.getCallbackRegistration(id: Long)
PUT     /customer/:id/callbackregistration       controllers.Register.putCallbackRegistration(id: Long)

POST    /token						controllers.OAuth2.postToken
POST    /invalidate_token			controllers.OAuth2.postInvalidateToken

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /webjars/*file              controllers.WebJarAssets.at(file)
